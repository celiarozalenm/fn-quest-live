# Quest Live Configuration

# Bubble API - Test Environment (version-test)
VITE_BUBBLE_API_URL_TEST=https://quest.fwd.app/version-test/api/1.1

# Bubble API - Live Environment (production)
VITE_BUBBLE_API_URL_LIVE=https://quest.fwd.app/api/1.1

# Bubble API Key (same key works for both environments)
VITE_BUBBLE_API_KEY=your-bubble-api-key

# Default environment: 'test' or 'live'
VITE_BUBBLE_ENV=test

# Auth0 Configuration
VITE_AUTH0_DOMAIN=userauth.forwardnetworks.com
VITE_AUTH0_CLIENT_ID=your-auth0-client-id
VITE_AUTH0_REDIRECT_URI=https://quest-live.pages.dev/callback

# Bubble Gateway (fallback if Auth0 redirect not configured)
VITE_USE_BUBBLE_GATEWAY=false
VITE_BUBBLE_GATEWAY_URL=https://quest.fwd.app/auth-gateway

# Admin Credentials
VITE_ADMIN_USERNAME=admin
VITE_ADMIN_PASSWORD=questlive2025

# Legacy Supabase (for original Quest admin)
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
